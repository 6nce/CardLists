{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "number": {
        "type": "string",
        "pattern": "^[0-9]+$"
      },
      "name": {
        "type": "string"
      },
      "attributes": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "note": {
        "type": "string",
        "optional": true
      },
      "variations": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "variation": {
              "type": "string"
            },
            "note": {
              "type": "string"
            }
          },
          "required": ["variation", "note"]
        },
        "optional": true
      }
    },
    "required": ["number", "name", "attributes"]
  }
}
